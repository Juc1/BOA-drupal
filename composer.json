{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/core-project-message": "^9.1",
        "drupal/core-recommended": "^9.1",
        "drupal/admin_toolbar": "^2.4",  
        "drupal/antibot": "^1.4",       
        "drupal/audiofield": "^1.10",
        "drupal/avatars": "^1.1",        
        "drupal/backup_migrate": "^5.0.0-rc2",
        "drupal/blazy": "^2.1",
        "drupal/checklistapi": "^2.0.0",
        "drupal/colorbutton": "^1.2",
        "drupal/comment_notify": "^1.2",
        "drupal/comments_order": "^1.1",
        "drupal/ctools": "^3.4",
        "drupal/devel": "^4.0.1",
        "drupal/easy_install": "^10.4",
        "drupal/entity_browser": "^2.5",
        "drupal/entity_usage": "^2.0-beta3",
        "drupal/extlink": "^1.5",
        "drupal/field_group": "^3.1",
        "drupal/flexslider": "^2.0",
        "drupal/feeds": "^3.0-alpha9",
        "drupal/geshifilter": "^2.0-beta1",
        "drupal/google_analytics": "^3.1",
        "drupal/google_tag": "^1.4",
        "drupal/honeypot": "^2.0.1",
        "drupal/imce": "^2.3",
        "drupal/masquerade": "^2.0-beta4 ", 
        "drupal/menu_item_extras": "^2.13",
        "drupal/metatag": "^1.15",
        "drupal/pager": "^1.4",
        "drupal/panelbutton": "^1.3",
        "drupal/pathauto": "^1.8",
        "drupal/paragraphs": "^1.12",
        "drupal/responsive_menu": "^4.4.1",
        "drupal/scheduler": "^1.3",
        "drupal/search_api": "^1.18",
        "drupal/simple_instagram_feed": "^3.9",
        "drupal/simple_sitemap": "^3.8",
        "drupal/slick": "^2.2",  
        "drupal/slick_views": "^2.3",      
        "drupal/smart_trim": "^1.3",
        "drupal/smtp": "^1.0",
        "drupal/token": "^1.9",
        "drupal/twig_field_value": "^2.0.0",
        "drupal/twig_tweak": "^3.0.0",
        "drupal/ultimenu": "^2.2",
	"drupal/viewsreference": "^2.0-beta2",
        "drupal/views_slideshow": "^4.8",
        "drupal/webform": "^6.0.0-beta3",
        "drupal/breeze": "^3.0.0",
        "drupal/bootstrap": "^3.23",
        "drupal/bootstrap_barrio": "^5.1.4"
    },
    "require-dev": {
        "drupal/core-dev": "^9.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            }
        },
        "installer-paths": {
            "core": ["type:drupal-core"],
            "libraries/{$name}": ["type:drupal-library"],
            "modules/contrib/{$name}": ["type:drupal-module"],
            "profiles/contrib/{$name}": ["type:drupal-profile"],
            "themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "modules/custom/{$name}": ["type:drupal-custom-module"],
            "profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "drupal-core-project-message": {
            "include-keys": ["homepage", "support"],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",

                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}